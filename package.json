{
  "name": "tick-tack-toe",
  "version": "0.0.0",
  "description": "A browser-based standard Tick Tack Toe game for 2 players using a split pane for",
  "main": "build/backend/main.js",
  "scripts": {
    "build-once": "babel src/backend --out-dir build/backend --source-maps --copy-files",
    "build": "babel src/backend --out-dir build/backend --source-maps --copy-files --watch",
    "webpack-once": "webpack --config ./webpack.config.js --progress --colors -d",
    "webpack": "webpack --config ./webpack.config.js --progress --colors -d --watch",
    "test": "mocha --compilers js:babel-core/register test/*/**/*.js",
    "debug": "babel-node debug build/backend/main.js",
    "start": "node build/backend/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mohammed-El-Nabulsi/Tick-Tack-Toe.git"
  },
  "keywords": [
    "tick-tack-toe",
    "game",
    "multiplayer"
  ],
  "author": "Mohammed El-Nabulsi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mohammed-El-Nabulsi/Tick-Tack-Toe/issues"
  },
  "homepage": "https://github.com/Mohammed-El-Nabulsi/Tick-Tack-Toe#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-template": "^5.0.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5",
    "source-map-support": "^0.4.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "flux": "^2.1.1",
    "node-graceful": "^0.2.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
