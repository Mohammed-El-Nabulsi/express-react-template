{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "__DEV__": true,
    "assert": true,
    "inject": true,
    "angular": true,
    "chai": true,
    "html": true,
    "css": true,
    "Promise": true,
    "Traders": true,
    "Users": true,
    "document": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "react/react-in-jsx-scope": 0,
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 0,
    "jsx-quotes": [2, "prefer-double"],
    "comma-dangle": [2, "always-multiline"],
    "array-bracket-spacing": [0, "never"],
    "indent": [1, "tab"],
    "react/jsx-indent": [2, "tab"],
    "no-alert": 0,
    "space-in-parens": 0,
    "no-use-before-define": 0,
    "object-curly-spacing": [2, "always"],
    "padded-blocks": 0,
    "no-restricted-syntax": [0, "ForInStatement"],
    "curly": 0,
    "quote-props": 0,
    "no-param-reassign": 0,
    "no-console": 0,
    "img-redundant-alt": 0,
    "key-spacing": 0,
    "no-underscore-dangle": 0,
    "array-callback-return": 0,
    "dot-notation": 0,
    "default-case": 0,
    "arrow-body-style": 0,
    "no-unused-vars": 0,
    "prefer-template": 0,
    "no-nested-ternary": 0,
    "import/no-extraneous-dependencies": ["error", { devDependencies: true }]
  },
  "env": {
    "mocha": true,
    "jasmine": true,
    "protractor": true
  }
}
